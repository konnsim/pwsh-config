{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "properties": {
            "style": "mixed"
          },
          "background": "#29315A",
          "foreground": "#3EC669",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "background": "#29315A",
          "foreground": "#43CCEA",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "status",
          "style": "plain",
          "template": "x ",
          "background": "",
          "foreground": "#C94A16",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ .Full }}",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true,
            "pnpm_icon": "<#B76507>\ue71e pnpm</> ",
            "npm_icon": "<#FE4A49>\ue71e npm</> ",
            "yarn_icon": "<#44FFD2>\uf487 yarn</> "
          },
          "background": "",
          "foreground": "#5fb157",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "crystal",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "background": "",
          "foreground": "#4063D8",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "ruby",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "background": "",
          "foreground": "#DE3F24",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "python",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": false
          },
          "background": "",
          "foreground": "#FED142",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "âžœ ",
          "background": "",
          "foreground": "#63F08C",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    }
  ]
}
